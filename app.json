{
  "expo": {
    "name": "Kielo.app",
    "slug": "kieloapp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/ic-launcher/icon.png",
    "scheme": "kieloapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kielo.app",
      "requireFullScreen": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "localhost": {
              "NSIncludesSubdomains": false,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/ic-launcher/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kielo.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/ic-launcher/web/favicon-96x96.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/ic-launcher/web/favicon-96x96.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da03360b-ad32-484c-ab92-ea26277e74d8"
      }
    }
  }
}
