{
  "name": "kielo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start -c",
    "start:tunnel": "expo start --tunnel",
    "web": "expo start --web",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "run:ios:simulator": "expo run:ios --simulator \"iPhone 16 Pro\"",
    "prebuild": "expo prebuild --clean",
    "build:local:prepare": "expo prebuild --clean",
    "build:local:ios:release": "echo 'Run build:local:prepare first, then open Xcode (ios/Kielo.xcworkspace) and Archive manually for Release.'",
    "build:local:android:release:apk": "echo 'Run build:local:prepare first, then run: cd android && ./gradlew assembleRelease && cd ..' && echo 'Find APK in android/app/build/outputs/apk/release/'",
    "build:local:android:release:aab": "echo 'Run build:local:prepare first, then run: cd android && ./gradlew bundleRelease && cd ..' && echo 'Find AAB in android/app/build/outputs/bundle/release/'",
    "build:eas:dev:ios": "eas build --platform ios --profile development",
    "build:eas:dev:android": "eas build --platform android --profile development",
    "build:eas:preview:ios": "eas build --platform ios --profile preview",
    "build:eas:preview:android": "eas build --platform android --profile preview",
    "build:eas:prod:ios": "eas build --platform ios --profile production",
    "build:eas:prod:android": "eas build --platform android --profile production",
    "submit:eas:ios": "eas submit --platform ios --profile production",
    "submit:eas:android": "eas submit --platform android --profile production",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "reset-project": "node ./scripts/reset-project.js",
    "clean:metro": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* || true",
    "clean:npm": "rm -rf node_modules && npm cache clean --force && npm install",
    "clean:pods": "if [ -d \"ios\" ]; then cd ios && rm -rf Pods Podfile.lock && pod install && cd ..; else echo \"iOS directory not found.\"; fi",
    "clean:android": "if [ -d \"android\" ]; then cd android && ./gradlew clean && cd ..; else echo \"Android directory not found.\"; fi",
    "clean:all": "npm run clean:metro && npm run clean:npm && npm run clean:pods && npm run clean:android && npm run prebuild"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "js-cookie",
          "normalizr",
          "transducers.js",
          "lucide-react-native"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.3.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.6.1",
    "expo": "~52.0.46",
    "expo-auth-session": "^6.0.3",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-crypto": "^14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-screen-orientation": "~8.0.4",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "js-cookie": "^3.0.5",
    "lucide-react-native": "^0.501.0",
    "normalizr": "^3.6.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-redux": "^9.2.0",
    "redux-devtools-expo-dev-plugin": "^1.0.0",
    "transducers.js": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
